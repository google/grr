    "provides": ["environ_windir"],
    "doc": "Test content_regex_list interpolation",
    "labels": ["Logs", "Authentication"],
    "supported_os": [ "Windows" ]
  },

  { "name": "DepsControlSet",
    "collectors": [
      { "action": "GetFiles",
        "args": { "path_list": ["/test/controlset"] }
      }
